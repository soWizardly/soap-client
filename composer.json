{
  "name": "khatfield/soap-client",
  "type": "library",
  "description": "A PHP client for the Salesforce SOAP API",
  "keywords": [
    "salesforce",
    "crm",
    "soap",
    "force.com",
    "web services"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Keith Hatfield",
      "email": "keith@keithhatfield.com",
      "homepage": "https://keithhatfield.com"
    },
    {
      "name": "David de Boer",
      "email": "david@ddeboer.nl",
      "homepage": "http://ddeboer.nl"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ext-soap": "*",
    "ext-json": "*",
    "ext-xml": "*",
    "psr/log": "*",
    "symfony/event-dispatcher": "^5",
    "doctrine/collections": "~1.6"
  },
  "require-dev": {
    "doctrine/common": ">=2.3",
    "phpunit/phpunit": "^7.0"
  },
  "suggest": {
    "doctrine/common": "For caching SOAP responses",
    "monolog/monolog": "For logging SOAP transactions"
  },
  "autoload": {
    "psr-4": {
      "Khatfield\\SoapClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Khatfield\\SoapClient\\Tests\\": "tests/"
    }
  }
}
